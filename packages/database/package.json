  "dependencies": {
    "@prisma/client": "^4.13.0"
  },
  "devDependencies": {
    "@formbricks/tsconfig": "workspace:*",
    "eslint": "^8.40.0",
    "eslint-config-formbricks": "workspace:*",
    "prisma": "^4.13.0",
    "prisma-dbml-generator": "^0.10.0",
    "rimraf": "^5.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  }
